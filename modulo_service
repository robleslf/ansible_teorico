ansible Ubuntu -m service -a "name=apache2 state=started" -b

ansible Ubuntu -m service -a "name=apache2 state=stopped" -b

ansible Ubuntu -m service -a "name=apache2 state=restarted" -b

ansible Ubuntu -m service -a "name=apache2 state=reloaded" -b

ansible Ubuntu -m service -a "name=apache2 enabled=yes" -b
